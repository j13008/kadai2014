<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <TextView
        android:id="@+id/winnerName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="56dp"
        android:layout_marginTop="49dp"
        android:text="TextView" />

    <Button
        android:id="@+id/resultviewbtn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/winnerName"
        android:layout_marginTop="26dp"
        android:text="戦績表示" />

    <Button
        android:id="@+id/titlebtn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/resultviewbtn"
        android:text="タイトル画面へ" />

    <ListView
        android:id="@+id/resulltlistView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/titlebtn"
        android:layout_marginLeft="14dp"
        android:layout_marginTop="18dp" >
    </ListView>



</RelativeLayout>
